d1 $(#) (vowel "i") $ fast (slow 2 "0.5 1 2 0.5") $ fast "  2 4 "
  $ rev $ sound "{sub:4 sub:6 <sub:7 sub:0> [<sub:9 sub:1> <sub:11 sub:9>]}%1"

d5 $ sometimes (hurry 2) $ sound "bsvocals*8?" # legato 0.5 # vowel (choose ["a","o","p"])

d6 $(#) (vowel "u") $ (#) (speed "0.5  1 ") $ sound "[db, 808bd:5]" # gain 1.2

-- this is session two:

d1 $every 4 (fast 2) $ degradeBy 0.5 $  jux (iter 4) $ sound " [~ bd] hh cp <[bd hh] [hh bd]>"

d3 $ every 4 (fast 0.5) $ degradeBy 0.5 $ iter 4 $ striate "<8 8 8 16 32>" $ sound "bsvocals:1 bsvocals/2" # legato 1

d6 $ every 4 (fast 0.5) $ sound "cpu1(3,8)"

hush

d4
  -- $(#) (speed "0.5  0.25 ")
  -- $ chop (fast 2 $ (+) "1 2" $ (+) (fast "0.5  0.25 " 2) "1 2") $ fast ((+) "0.5  0.25 " "  2 4 ")
  $ rev $ (#) (fast "0.5 1 2 0.5" $ rev $ vowel "i") $ sound "dr55*8?"


d5 $fast "  2 4 " $ chop (rev $ fast "  2 4 " 2) $ sound " {[saw1:2 saw1:4] saw1:7*2 [~ saw:9] saw:7 saw1:12}%4" # legato 0.5

d5 silence

