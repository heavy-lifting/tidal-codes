d1 $ fast 4 $ jux (iter 4) $ sound "bshihat:1(7,16)" # legato 0.5

d2 $ slow 4 $ every 4(slow 2) $ sound "[cpu*4, cpu1(<3 5 6>,8)]"

d3 $ slow 8 $ every 4 (fast 2) $ jux (iter 2) $ chunk 2 (hurry "[0.5,2]") $ sound "vkl:1 vkl:1/3" # legato 0.25 # up (choose [0,5,7,12]) # speed "[0.5,1]"

d4 $ every 4 (fast 2) $ jux (chop "<8 4>") $ loopAt 2 $ chop "<4 8>" $ sound "isa*4?" # legato 0.5

d5 $ slow 2 $ jux (fast 2) $ sound "{cpu:4*3 ~, ~ bssnare*4 ~}"

