-- this was session one

setcps (157/60/4)

hush

d4 $ iter 4
  -- $(#) (fast "  2 4 "
  -- $ slow ((+) (slow "  2 4 " "0.5  0.25 ") "  2 4 ") $ speed "0.5  1 ")
  $ sound "{hsaw:12 ~ ~ hsaw:0, [~ hsaw:6] [~ hsaw:9] [~ hsaw:0] [~ hsaw:7] }" # legato 0.5

d2 $ every 2 ( fast "<2 2 0.5>") $ jux (iter 4)
-- $(#) ((#) (vowel "o") $ fast (rev "0.5  1 ") $ speed "  2 4 ")
  $ sound "~ bsperc:6*4 [~ sn:5] cp:1"
