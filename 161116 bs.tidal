c1 $ loopAt 2 $ sound "bsbass"
c1 silence

c2 $ sound "bskick*2 bskick/2" # gain "1.2" # vowel "a o a"
c2 silence

c3 $ every 2 (slow 2) $ loopAt 0.5 $ sound "bsguitar*2 bsguitar:1" # iter 3 (up "-1 -3 -5")
c3 silence

c4 $ sound "[bshihat bssnare] [bssnare*3]  ~ ~" # pan "1 0 1" # delay "0.2"
c4 silence

hush
