d1 $ sometimesBy 0.125 (density 8) $ sound "bskick:1*4" # shape "0.4"

d2 silence

setcps 0.4

hush

d2  $ jux (fast 2.iter 4 ) $ n (run 4 + "<1 2 3 4 5>") |*| sound "cpu4(3,8)" # shape "0.4"

d3 $ density "<0.5 1 8 2 4 16>" $ loopAt "<1 2 0.5 4>" $ chop "<16 [32 64]>" $ sound "vkloop:1*4?" # cut 1

d4 $ jux (hurry 0.5) $ sound "vkb(7,16)" # iter 4 (speed "1 1 2 0.5") # cut 2
