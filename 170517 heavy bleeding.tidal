c1 $ sound "{~ bsperc:3*2 ~ ~ ~, bskick:1}" # cut 1

c1 silence

c2 $ jux (# vowel "<a o a>") $ slowspread (loopAt) [10,9,8] $ chop 32 $ sound "bsnoise:15 bsnoise:15"

c2 silence

c3 $ speed "[1 2 [1[1 2 ] 1[2 1] 2] 1[1 [2 ]1 ]2 1]" # sound "noise:16"

c3 silence

