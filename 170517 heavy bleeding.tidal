--- this jam goes along with various sectons - use cps 1.1 for all!!!!!!

cps 1.1

c1 $ jux (rev) $ every 4 (density 4) $ sound "{~ ~ ~ ~, [bskick:1 bshihat:1]}" # cut 1

c1 silence

c2 $ jux (density 2) $ sound "~ [~ bsperc:3] ~"

c2 silence

c3 $ slowspread (loopAt) [16,8] $ degradeBy 0.5 $ chop 32 $ n (run 4) $ sound "bsvocals:1"

c4 $ degradeBy 1 $ jux (density 2) $ iter 2 $ sound "~ bsperc:2" # up "<-5 -7 -3>" # vowel "<a o>"

hush

