setcps 1.15

d8 $ chunk 3 (hurry 2) $
jux (slow 2) $ sound "{9rs*8?, 9bd:1 9bd:1*2, ~ 9cp/2 ~, ~ ~ 9hh 9hh*2 9hh*3}" # shape 0.2
# pan (slow 2 $ sine)

d3 $ degradeBy "<0 0 0 0.25>" $ up (scale "locrian" "<5 7 5 12>" - 12) # sound "vkb" # legato 0.5

d4 $ sound "~ bsbass:6"

d5 $ sound "kick kick/2"

d9 $ bump "hi" 8 "{f!11 t}%8" $
jux (iter 4) $ chunk 8 (# gain 0) $ somecyclesBy 0.25 (slow 2)
$ up (scale "locrian" "1 7 3 5" - 12)
