d2 $ sound "hbird:1*16?" # up (scale "locrian" (irand 8))

hush

d2 $ striate 2 $ loopAt "[2]" $ n " <0 1 1 2> <4 5>" # sound "cry" # shape 0.4 # legato 1

d1 -- $ jux rev
  $ slow 4
  -- $ slow (6 * 12 / 8)
  $ squeeze "0@4 0@3 0@2 0 0 0" [sound "bd sd ~ ht mt lt"]
